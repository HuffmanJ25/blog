<div class="mdl-grid contact-container">
  <div class="mdl-cell mdl-cell--1-offset-desktop mdl-cell--10-col-desktop mdl-cell--8-col-tablet contact-form">
    <div class="contact-form-wrapper">
      <h2 class="contact-form-heading">Reach out to me about anything!</h2>
      <%= form_tag '/contact_email' do %>
      <div class="mdl-textfield mdl-js-textfield">
        <%= label_tag 'body', "I would love to hear from you!", class: 'mdl-textfield__label' %><br />
        <%= text_area_tag 'body', nil, class: "mdl-textfield__input" %>
      </div><br />
      <div class="mdl-textfield mdl-js-textfield">
        <%= label_tag :name, "What is your name?", class: "mdl-textfield__label" %>
        <%= text_field_tag :name, nil, class: "mdl-textfield__input" %>
      </div><br />
      <div class="mdl-textfield mdl-js-textfield">
        <%= label_tag :email, "What is your Email?", class: "mdl-textfield__label" %>
        <%= email_field_tag :email, nil, class: "mdl-textfield__input" %>
      </div><br />
      <%= submit_tag 'Send Message', class: 'mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent' %>
      <% end %>
    </div>
  </div>
  <div class="mdl-cell mdl-cell--3-offset-desktop mdl-cell--2-col-desktop mdl-cell--1-offset-tablet mdl-cell--2-col-tablet contact-link">
